{
    "name": "ocdladefense/ocdlaprod",
    "description": "A wordpress install",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config":{
        "preferred-install":{
            "ocdladefense/*":"source"
        }
    },
    "extra": {
    	"installer-paths": {
            "wp-content/plugins/committees":["ocdladefense/wp-committees"],
            "wp-content/plugins/donors":["ocdladefense/wp-donors"],
            "wp-content/plugins/salesforce":["ocdladefense/wp-salesforce"],
            "wp-content/plugins/ccpay":["ocdladefense/wp-plugin-ccpay"]
    	}
    },
    "require": {
        "composer/installers":"~1.0",
        "ocdladefense/wp-committees":"~1.0",
        "ocdladefense/wp-salesforce":"~2.0",
        "ocdladefense/wp-plugin-ccpay":"dev-development",
        "wpackagist-plugin/create-and-assign-categories-for-pages":"1.2",
        "wpackagist-plugin/custom-post-widget":"3.2.1",
        "wpackagist-plugin/custom-sidebars":"3.31",
        "wpackagist-plugin/wp-media-manager-lite":"1.1.2"
    }
}